#include <stdio.h>

int main() {
    int a[10][10], r, c;
    int isSym = 1;

    printf("Enter rows and columns: ");
    scanf("%d %d", &r, &c);

    if(r != c) {
        printf("FALSE");   // Not a square matrix â†’ cannot be symmetric
        return 0;
    }

    printf("Enter the matrix:\n");
    for(int i = 0; i < r; i++) {
        for(int j = 0; j < c; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    // Check symmetry
    for(int i = 0; i < r; i++) {
        for(int j = 0; j < c; j++) {
            if(a[i][j] != a[j][i]) {
                isSym = 0;
                break;
            }
        }
    }

    if(isSym)
        printf("TRUE");
    else
        printf("FALSE");

    return 0;
}
