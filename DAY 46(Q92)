#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    int i, j;

    printf("Enter a string: ");
    scanf("%s", s);

    for (i = 0; i < strlen(s); i++) {
        for (j = i + 1; j < strlen(s); j++) {
            if (s[i] == s[j]) {
                printf("First repeating lowercase alphabet: %c\n", s[i]);
                return 0;
            }
        }
    }

    printf("No repeating lowercase alphabet found.\n");
    return 0;
}
