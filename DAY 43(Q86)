#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i;

    printf("Enter a string: ");
    gets(str);

    for (i = 0; i < strlen(str); i++) {
        if (str[i] != str[strlen(str) - i - 1]) {
            printf("Not Palindrome");
            return 0;
        }
    }

    printf("Palindrome");
    return 0;
}
