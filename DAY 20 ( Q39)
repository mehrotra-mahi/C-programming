#include <stdio.h>

int main() {
    int num, digit;
    int product = 1;

    printf("Enter a number: ");
    scanf("%d", &num);

    if (num == 0) {
        printf("No odd digits, product = 0\n");
        return 0;
    }

    while (num > 0) {
        digit = num % 10;   
        if (digit % 2 != 0) {  
            product *= digit;
        }
        num /= 10;  // remove last digit
    }

    if (product == 1)  
        printf("No odd digits, product = 0\n");
    else
        printf("Product of odd digits = %d\n", product);

    return 0;
}
